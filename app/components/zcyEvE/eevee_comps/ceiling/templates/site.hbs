<div class="select-box last" style="width: {{width}}px;left:{{left}}px;" >
  {{#each items}}
    <div class="content-box " style="width:75px"  >
      <span class="title eevee-word" style="text-align: left;padding-left: 10px;">{{tagName}}</span >
      {{#each children}}
        <span ><a target="_blank" href="{{href}}" class="eevee-word">{{tagName}}</a ></span >
      {{/each}}
    </div >
  {{/each}}
</div >
