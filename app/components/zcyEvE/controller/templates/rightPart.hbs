<div class="right-button"><button class="btn btn-primary btn-medium addpage">增加页面</button></div>
<ul class="right-page-ul">
  {{#each this}}
    <li class="show-title" data-page="{{this.page}}" data-pagename="{{this.pageName}}">
      <div class="page-title">{{this.pageName}}</div>
      <div>
        <a class="page-link">{{this.page}}</a>
        <a class="delete-page">x</a>
      </div>
    </li>
  {{/each}}
</ul>
