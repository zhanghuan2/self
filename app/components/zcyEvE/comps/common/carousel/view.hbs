{{#component "homepage-carousel-container evees-comp js-comp"}}
  <div class="carousel{{#equals isSuper "1"}} carousel-super{{/equals}}{{#equals type "round"}} carousel-rounded{{/equals}}{{#equals type "square"}} carousel-squared{{/equals}}{{#equals type "round-rt"}} carousel-rounded-rt{{/equals}}{{#equals type "square-lb"}} carousel-squared-lb{{/equals}}" 
       data-ride="carousel" 
       data-type="switchable" 
       data-interval="{{#if interval}}{{interval}}{{else}}3000{{/if}}"
       data-row-size="{{rowSize}}" 
       style="height:{{#if height}}{{height}}{{else}}378{{/if}}px;">

    <!-- Wrapper for slides -->
    <div class="carousel-contents">
      {{#if images}}
        {{#each images}}
        <div data-role="content" class="carousel-content {{#unless @first}}hide{{/unless}}" data-id="1" 
              style="height:{{#if ../height}}{{../height}}{{else}}378{{/if}}px;
              background-image:url('{{image}}'); background-repeat:no-repeat; background-position: center center; background-size: 718px 378px;">
          <a {{#if href}}href="{{href}}"{{/if}} target="_blank"></a>
        </div>
        {{/each}}
        {{else}}
        <div data-role="content" class="carousel-content" data-id="1"
             style="height:{{#if ../height}}{{../height}}{{else}}378{{/if}}px;
                 background-image:url('/assets/images/other-images/banner02.png'); background-repeat:no-repeat; background-position: center center; background-size: 718px 378px;">
          <a {{#if href}}href="{{href}}"{{/if}} target="_blank"></a>
        </div>
        <div data-role="content" class="carousel-content hide" data-id="1"
             style="height:{{#if ../height}}{{../height}}{{else}}378{{/if}}px;
                 background-image:url('/assets/images/other-images/banner03.png'); background-repeat:no-repeat; background-position: center center; background-size: 718px 378px;">
          <a {{#if href}}href="{{href}}"{{/if}} target="_blank"></a>
        </div>
      {{/if}}
    </div>
    <div class="carousel-navs" data-role="link">
      <!-- Indicators -->
      {{#equals isIndicator "1"}}
        <ol class="carousel-indicators">
          {{#if images}}
            {{#each images}}
              <li data-role="nav" {{#if @first}}class="active"{{/if}}>
                {{#if ../type}}
                  {{#equals ../type "auto"}}
                  {{else}}
                    <span class="icon-zcy icon-lunbo-yuandian"></span>
                    <span class="icon-zcy icon-lunbo-yuandian-active"></span>
                  {{/equals}}
                {{/if}}
              </li>
            {{/each}}
          {{else}}
            {{#if image1}}<li data-role="nav" class="active">{{#if type}}{{#equals type "auto"}}{{else}}<span class="icon-zcy icon-lunbo-yuandian"></span><span class="icon-zcy icon-lunbo-yuandian-active"></span>{{/equals}}{{/if}}</li>{{/if}}
            {{#if image2}}<li data-role="nav">{{#if type}}{{#equals type "auto"}}{{else}}<span class="icon-zcy icon-lunbo-yuandian"></span><span class="icon-zcy icon-lunbo-yuandian-active"></span>{{/equals}}{{/if}}</li>{{/if}}
            {{#if image3}}<li data-role="nav">{{#if type}}{{#equals type "auto"}}{{else}}<span class="icon-zcy icon-lunbo-yuandian"></span><span class="icon-zcy icon-lunbo-yuandian-active"></span>{{/equals}}{{/if}}</li>{{/if}}
            {{#if image4}}<li data-role="nav">{{#if type}}{{#equals type "auto"}}{{else}}<span class="icon-zcy icon-lunbo-yuandian"></span><span class="icon-zcy icon-lunbo-yuandian-active"></span>{{/equals}}{{/if}}</li>{{/if}}
            {{#if image5}}<li data-role="nav">{{#if type}}{{#equals type "auto"}}{{else}}<span class="icon-zcy icon-lunbo-yuandian"></span><span class="icon-zcy icon-lunbo-yuandian-active"></span>{{/equals}}{{/if}}</li>{{/if}}
          {{/if}}
        </ol>
      {{/equals}}

        <!-- Controls -->
      {{#equals isControl "1"}}
        <a class="left carousel-control" data-role="prev">
          <i class="icon-carousel-left"></i>
        </a>
        <a class="right carousel-control" data-role="next">
          <i class="icon-carousel-right"></i>
        </a>
      {{/equals}}
    </div>
  </div>
{{/component}}
